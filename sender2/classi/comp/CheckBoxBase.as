package classi.comp{	import classi.ClientStarter;	public class CheckBoxBase extends ButtonTxt {		var txt;		///////		public var myStatus;		public function CheckBoxBase(xx:Number,yy:Number,ww,hh,t:String,f:Function,alt:String,myStat:Boolean) {			super(xx,yy,ww,hh,t,f,null,alt);			myStatus = !myStat;			myStatus_swap();			txt = t			fnz = f			if (lab.textWidth > ww) {				lab.text="";			}		}		override function mouseOverHandler(event):void {			trace("mouseOverHandler" + this);			if (lab.text=="") {				lab.text=txt + myTxt;				lab.width=lab.textWidth + 4;				this.parent.swapChildren(this, this.parent.getChildAt(this.parent.numChildren-1))			}			if (ClientStarter.vKS && alt) {				//avviaAlt(alt);			}		}		override function mouseOutHandler(event):void {			trace("mouseOutHandler");			if (lab.textWidth > w) {				lab.text="";				lab.width=w;			}			if (ClientStarter.vKS && alt) {				//stopAlt();			}		}		override function mouseDownHandler(event):void {			trace("mouseDownHandler");			myStatus_swap();			fnz(myStatus);			//q1!			//Selection.setFocus(null);			//mouseOutHandler(event);		}		public function myStatus_swap() {			if (myStatus) {				myStatus=false;				myTxt=" OFF";				lab.backgroundColor=0xFFFFFF;			} else {				myStatus=true;				myTxt=" ON";				lab.backgroundColor=0xFF0000;			}		}	}}