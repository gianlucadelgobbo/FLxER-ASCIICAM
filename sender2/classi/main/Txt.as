package classi.main{	import flash.text.TextField;	import flash.text.TextField.defaultTextFormat 		public class Txt extends TextField{		public function Txt(xx:Number,yy:Number,ww,hh,t:String,tf) {			x = xx			y = yy			//border = true			text=t;			embedFonts=true;			setTextFormat(tf);			defaultTextFormat = tf			if (ww != null) {				width = ww;			} else {				width=textWidth + 9;			}			if (hh != null) {				height = hh;			} else {				height=textHeight + 4;			}			selectable=false;		}	}}