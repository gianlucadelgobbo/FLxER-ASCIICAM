package classi.main{	import flash.display.*;	import flash.text.*;	import flash.text.TextField.defaultTextFormat 		public class TxtStyle{		public static var ts;		public static var th:TextFormat;				public function TxtStyle() {		}		public static function startTs() {			var embeddedFontsArray:Array = Font.enumerateFonts(false);			trace (embeddedFontsArray[0].fontName)			ts = new TextFormat();			with (ts) {				font = embeddedFontsArray[1].fontName;				size = 10;				color = 0x000000;				leading = -2;				leftMargin = 1;				rightMargin = 0;			}			th = new TextFormat();			with (th) {				font = embeddedFontsArray[0].fontName;				size = 7;				color = 0x00FF00;				leading = -2;				leftMargin = 1;				rightMargin = 0;			}			//return th;		}	}}