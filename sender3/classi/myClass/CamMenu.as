package classi.myClass{	import flash.display.*;	import flash.media.*;	import flash.xml.XMLDocument;	import flash.xml.XMLNode;		import classi.comp.*;	import classi.main.*;			public class CamMenu extends Sprite{				public var menu;		public var selCamBtn;		public var selConBtn;		public var sMenu;		public var voceMen; 		public var cameraList;		public var connList;		public var camere;		public var conn;		public var listaConn;		public var listaCam;		public function CamMenu() {									this.menu = new Rett(0, 0, 321, 19, 0xCCCCCC, 0.1, 100);			this.addChild(menu);						xmlMake()						}										public function xmlMake(){				cameraList = new XMLDocument();				cameraList.ignoreWhite = true;								connList = new XMLDocument();				connList.ignoreWhite = true;								listaConn="<conn>";				listaConn+="<a m=\"c1\" /><a m=\"c2\" /><a m=\"c3\" /><a m=\"c4\" />";				listaConn+="</conn>";				listaCam ="<cam>";				var cont = 0;				while(cont < Camera.names.length){					listaCam+= "<li m=\"" + Camera.names[cont] + "\" val=\""+ cont +"\" />"					cont ++;					}				listaCam +="</cam>";				cameraList.parseXML(listaCam);				trace (cameraList.childNodes[0].childNodes[1].attributes.val + " XML")								connList.parseXML(listaConn);				trace (listaConn + " XML")												this.camere = new ListMenu(2,2,100,15,"CAMERA", newCamSel, "camera", cameraList, cameraList.childNodes[0].childNodes.length)				this.menu.addChild(camere)								this.conn = new ListMenu(102,2,100,15,"CONNESSIONE", newConnSel, "connessioni", connList, connList.childNodes[0].childNodes.length)				this.menu.addChild(conn)				}							public function newCamSel(val){			parent.cameraIndex = val;			parent.wcam.newCam(val, parent.connName )						}		public function newConnSel(val){			parent.connName = val;			parent.wcam.newCam(parent.cameraIndex, parent.connName )						}		}}