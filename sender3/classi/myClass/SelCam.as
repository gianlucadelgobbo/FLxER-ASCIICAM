package classi.myClass{	import flash.display.*;	import flash.media.*;		import classi.comp.*;	import classi.main.*;		public class SelCam extends Sprite{		public static var ts;		trace (ClientStarter.numWc + " numWC")		public function SelCam() {			trace("SelCam" + this)						btn1 = new ButtonTxt(10, 30, null, null, "+",newCam, "n", "alt:String")			this.addChild(btn1)															}					public function newCam(n){			rett = new Rett(10, 65, 400, 210, 0xff0000, null, 100);			this.addChild(rett);			var cont = 0;			while(cont < Camera.names.length){								pippo = new ButtonTxt(10, 30+(30*cont), null, null, Camera.names[cont],newCamSel, cont, "alt:String")				rett.addChild(pippo)				//trace (parent.numWc + " numWC")				trace (cont + " " + Camera.names[cont])				cont++				}						}		public function newCamSel(val){			trace ("newCamSel " + val )			removeChild(rett)						wcam=new NetVideo("flux"+ClientStarter.numWc,val);			this.addChild(wcam);			wcam.y = 300			wcam.x = 25+(400*ClientStarter.numWc);			ClientStarter.numWc++;						}		}}