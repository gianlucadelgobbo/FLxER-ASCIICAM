package classi.comp {	import flash.display.Sprite;	import flash.events.MouseEvent;	import classi.comp.*;	import classi.main.DrawerFunc;	import classi.main.Txt;	import flash.text.TextField;	import classi.ClientStarter;	public class ButtonArrow extends Sprite {		var r:Number;		var fnz:Function;		var param:String;		var alt:String;		var col;		///////		function ButtonArrow(xx:Number, yy:Number, r, f:Function, p, a:String) {			fnz = f;			param = p;			alt = a;			rotation = r;			if (r == 180) {				x = xx+10;				y = yy+6;			} else if (r == -90) {				x = xx;				y = yy+10;			} else if (r == 90) {				x = xx+6;				y = yy;			} else {				x = xx;				y = yy;			}			DrawerFunc.arrowDrawer(this, 0x000000);			this.addEventListener(MouseEvent.MOUSE_OVER,mouseOverHandler);			this.addEventListener(MouseEvent.MOUSE_OUT,mouseOutHandler);			this.addEventListener(MouseEvent.MOUSE_DOWN,mouseDownHandler);			this.addEventListener(MouseEvent.MOUSE_UP,mouseUpHandler);			this.buttonMode=true;		}		function mouseOverHandler(event):void {			trace("mouseOverHandler" + this);			if (ClientStarter.vKS && alt) {				//avviaAlt(alt);			}		}		function mouseOutHandler(event):void {			trace("mouseOutHandler");			if (ClientStarter.vKS && alt) {				//stopAlt();			}		}		function mouseDownHandler(event):void {			trace("mouseDownHandler"+param);			fnz(parseInt(param));			//q1!			//Selection.setFocus(null);			mouseOutHandler(event);		}		function mouseUpHandler(event):void {			trace("mouseUpHandler");			fnz(null);		}	}}